# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Primary Workspace (Default)
BITBUCKET_WORKSPACE=workspace1
BITBUCKET_USERNAME=username1
BITBUCKET_APP_PASSWORD=app_password1

# Secondary Workspace (Optional)
BITBUCKET_WORKSPACE_2=workspace2
BITBUCKET_USERNAME_2=username2
BITBUCKET_APP_PASSWORD_2=app_password2

# Third Workspace (Optional)
BITBUCKET_WORKSPACE_3=workspace3
BITBUCKET_USERNAME_3=username3
BITBUCKET_APP_PASSWORD_3=app_password3

# Webhook Security
WEBHOOK_SECRET=your_webhook_secret_here

# Server Configuration
PORT=3002
NODE_ENV=production

# Feature Flags
ENABLE_MULTI_WORKSPACE=true
ENABLE_CUSTOM_CONFIGS=true
ENABLE_PATH_FILTERING=true