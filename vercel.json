{
  "version": 2,
  "name": "gme-code-review-bot",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["icn1"],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}